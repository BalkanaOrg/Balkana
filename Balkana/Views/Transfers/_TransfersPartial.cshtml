@{
}

@*Foreach
    <tr>
        <th scope="col">#</th>
        <th scope="col">Game Id</th>
        <th scope="col">Player Id</th>
        <th scope="col">Team Id</th>
        <th scope="col">Transfer date</th>
        <th scope="col">PositionId</th>
    </tr>*@
    @foreach (var tr in Model)
    {

        <tr onclick="window.location.href='transfers/details/@tr.Id'">
            <th scope="col">@tr.Id</th>
            <th><img src="@tr.GameName" width="30px" height="30px"/></th>
            <th>@tr.PlayerUsername</th>
            <th>@tr.TeamFullName</th>
            <th>@tr.TransferDate</th>
            <th>@tr.Position</th>
            <th>✍</th>
        </tr>
        
    }